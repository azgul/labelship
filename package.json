{
  "name": "labelship",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "dev:remix": "remix vite:dev",
    "dev:worker": "tsx watch workers/tracking.worker.ts",
    "docker-start": "npm run setup && remix-serve ./build/server/index.js",
    "setup": "prisma generate && prisma migrate deploy",
    "generate": "prisma generate",
    "deploy": "fly deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "worker": "tsx workers/tracking.worker.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "@remix-run/dev": "^2.16.0",
    "@remix-run/node": "^2.16.0",
    "@remix-run/react": "^2.16.0",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^13.9.0",
    "@shopify/shopify-app-remix": "^3.5.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.9",
    "bullmq": "^5.34.0",
    "isbot": "^5.1.0",
    "prisma": "^6.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-tsconfig-paths": "^4.3.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.16.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
